#!name=Airport Information 2
#!desc=Airport Information

[Panel]
# 机场1
Sub_info = script-name=Atomlantis,update-interval =-1
# 机场2
Sub_info2 = script-name=Amytelecom,update-interval =-1


[Script]
# 机场1
Atomlantis = type=generic,timeout=10,script-path=https://raw.githubusercontent.com/mubdao/Profiles/main/sgmodule/panel/sub-info/airport2.js,script-update-interval=-1,argument=url=encode后的机场节点链接&reset_day=20&title=𝗔𝘁𝗼𝗺𝗹𝗮𝗻𝘁𝗶𝘀
# 机场2
Amytelecom = type=generic,timeout=10,script-path=https://raw.githubusercontent.com/mubdao/Profiles/main/sgmodule/panel/sub-info/airport2.js,script-update-interval=-1,argument=url=encode后的机场节点链接&reset_day=18&title=𝗔𝗺𝘆𝘁𝗲𝗹𝗲𝗰𝗼𝗺
